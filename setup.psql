--
-- Cr5 Database Setup
-- 

CREATE TABLE counters (name VARCHAR UNIQUE NOT NULL, value NUMERIC NOT NULL);
CREATE TABLE ideas (guild_id BIGINT NOT NULL, idea_id INT NOT NULL, message_id BIGINT NOT NULL, author_id BIGINT);
CREATE TABLE idea_channel (guild_id BIGINT UNIQUE NOT NULL, channel_id BIGINT NOT NULL);
CREATE TABLE prefixes (guild_id BIGINT UNIQUE NOT NULL, prefix VARCHAR NOT NULL);
CREATE TABLE warns (guild_id BIGINT NOT NULL, user_id BIGINT NOT NULL, MODERATOR_ID BIGINT NOT NULL, reason VARCHAR);
